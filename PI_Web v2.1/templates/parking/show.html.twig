{% extends'baseBack.html.twig' %}
{% block body %}
<div class="content-wrapper">
    <div class="container-fluid">

        <div class="row mt-3">
            <div class="col-lg-6">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">Liste des parkings</h5>
                        <div class="table-responsive">
                            <table class="table table-striped">

        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ parking.id }}</td>
            </tr>
            <tr>
                <th>Nom_parking</th>
                <td>{{ parking.nomParking }}</td>
            </tr>
            <tr>
                <th>Nbr_place</th>
                <td>{{ parking.nbrPlace }}</td>
            </tr>
            <tr>
                <th>Latitude_parking</th>
                <td>{{ parking.latitudeParking }}</td>
            </tr>
            <tr>
                <th>Longitude_parking</th>
                <td>{{ parking.longitudeParking }}</td>
            </tr>
        </tbody>
    </table>


                            <br>
                            <button class="btn btn-outline-danger">
                                <a href="{{ path('app_parking_edit', {'id': parking.id}) }}"></a>
                                    <i class="fa fa-pencil" aria-hidden="true"></i></a>

                                {{ include('parking/_delete_form.html.twig') }}


                        </div>

                    </div>
                    <a href="{{ path('app_parking') }}">

                        <i class="fa fa-arrow-left" aria-hidden="true"></i>

                    </a>
                    <br>
                </div>

            </div>

        </div>
    </div>


{% endblock %}
